{
  "main": "dist/index.js",
  "name": "importmap-manager",
  "type": "module",
  "version": "0.0.9",
  "description": "A modern UI for managing import-map-overrides with an enhanced user experience.",
  "author": "Daniel David Díaz González <ddavid930826@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ddavid93/importmap-manager"
  },
  "scripts": {
    "analyze": "npx vite-bundle-visualizer",
    "build": "run-p type-check build-only",
    "build-only": "vite build --watch",
    "build-serve": "run-p build-only & serve ./dist",
    "dev": "vite",
    "format": "prettier --write src/",
    "install-packages": "pnpm install",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "prepublishOnly": "npm run build-only",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit --composite false",
    "update-packages": "pnpm upgrade --latest"
  },
  "dependencies": {
    "@vueuse/components": "13.0.0",
    "@vueuse/core": "13.0.0",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "lucide-vue-next": "0.484.0",
    "reka-ui": "2.1.1",
    "tailwind-merge": "3.0.2",
    "tailwindcss-animate": "1.0.7",
    "vue": "3.5.13"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.11.0",
    "@tsconfig/node20": "20.1.5",
    "@types/node": "22.13.13",
    "@vitejs/plugin-vue": "5.2.3",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/eslint-config-typescript": "14.5.0",
    "@vue/tsconfig": "0.7.0",
    "autoprefixer": "10.4.21",
    "eslint": "9.23.0",
    "eslint-plugin-vue": "10.0.0",
    "npm-run-all2": "7.0.2",
    "postcss": "8.5.3",
    "prettier": "3.5.3",
    "tailwindcss": "3.4.17",
    "typescript": "5.8.2",
    "vite": "6.2.3",
    "vue-tsc": "2.2.8"
  },
  "files": [
    "README.md",
    "dist",
    "index.js"
  ]
